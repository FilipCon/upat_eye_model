@article{Valero-Cuevas2003a,
abstract = {A biomechanical model of the thumb can help researchers and clinicians understand the clinical problem of how anatomical variability contributes to the variability of outcomes of surgeries to restore thumb function. We lack a realistic biomechanical model of the thumb because of the variability/uncertainty of musculoskeletal parameters, the multiple proposed kinematic descriptions and methods to solve the muscle redundancy problem, and the paucity of data to validate the model with in vivo coordination patterns and force output. We performed a multi-stage validation of a biomechanical computer model against our measurements of maximal static thumbtip force and fine-wire electromyograms (EMG) from 8 thumb muscles in each of five orthogonal directions in key and opposition pinch postures. A low-friction point-contact at the thumbtip ensured that subjects did not produce thumbtip torques during force production. The 3-D, 8-muscle biomechanical thumb model uses a 5-axis kinematic description with orthogonal and intersecting axes of rotation at the carpometacarpal and metacarpophalangeal joints. We represented the 50 musculoskeletal parameters of the model as stochastic variables based on experimental data, and ran Monte Carlo simulations in the "inverse" and "forward" directions for 5000 random instantiations of the model. Two inverse simulations (predicting the distribution of maximal static thumbtip forces and the muscle activations that maximized force) showed that: the model reproduces at most 50{\%} of the 80 EMG distributions recorded (eight muscle excitations in 5 force directions in two postures); and well-directed thumbtip forces of adequate magnitude are predicted only if accompanied by unrealistically large thumbtip torques (0.64??0.28Nm). The forward simulation (which fed the experimental distributions of EMG through random instantiations of the model) resulted in misdirected thumbtip force vectors (within 74.3??24.5?? from the desired direction) accompanied by doubly large thumbtip torques (1.32??0.95Nm). Taken together, our results suggest that the variability and uncertainty of musculoskeletal parameters and the choice of solution method are not the likely reason for the unrealistic predictions obtained. Rather, the kinematic description of the thumb we used is not representative of the transformation of net joint torques into thumbtip forces/torques in the human thumb. Future efforts should focus on validating alternative kinematic descriptions of the thumb. ?? 2003 Elsevier Science Ltd. All rights reserved.},
author = {Valero-Cuevas, Francisco J. and Johanson, M. Elise and Towles, Joseph D.},
doi = {10.1016/S0021-9290(03)00061-7},
file = {:C$\backslash$:/Users/mitkof6/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Valero-Cuevas, Johanson, Towles - Towards a realistic biomechanical model of the thumb The choice of kinematic description may be mor(2).pdf:pdf},
isbn = {1605255122},
issn = {00219290},
journal = {Journal of Biomechanics},
keywords = {Biochemical model,EMG,Hand,Monte Carlo simulation,Muscle coordination,Thumb,abbreviations,abductor pollicis brevis muscle,abductor pollicis longus muscle,add,adductor pollicis muscle,apb,apl,biochemical model,carpometacarpal,cmc,dio,electromyographic recordings,emg,epb,epl,extensor pollicis brevis muscle,extensor pollicis longus,first dorsal interosseous muscle,flexor pollicis brevis muscle,flexor pollicis longus muscle,fpb,fpl,hand,interphalangeal,ip,linear programming,lp,monte carlo simulation,mp,muscle,muscle coordination,thumb},
number = {7},
pages = {1019--1030},
pmid = {12757811},
title = {{Towards a realistic biomechanical model of the thumb: The choice of kinematic description may be more critical than the solution method or the variability/uncertainty of musculoskeletal parameters}},
volume = {36},
year = {2003}
}
