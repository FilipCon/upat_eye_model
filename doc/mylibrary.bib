isbn = {0021929011007},
issn = {00219290},
journal = {Journal of Biomechanics},
keywords = {Device evaluation,Device mechanics,Joint biomechanics,Musculoskeletal system,Standards in modeling,Tissue mechanics,standards in modeling},
number = {4},
