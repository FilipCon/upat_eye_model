file = {:C$\backslash$:/Users/mitkof6/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Valero-Cuevas - Predictive Modulation of Muscle Coordination Pattern Magnitude Scales Fingertip Force Magnitude Over the Voluntary Ra(3).pdf:pdf},
isbn = {0022-3077 (Print)},
issn = {0022-3077},
journal = {J Neurophysiol},
number = {3},
pages = {1469--1479},
pmid = {10712473},
title = {{Predictive Modulation of Muscle Coordination Pattern Magnitude Scales Fingertip Force Magnitude Over the Voluntary Range}},
volume = {83},
