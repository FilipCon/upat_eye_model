keywords = {Biological,Computer Simulation,Electromyography,Electromyography: instrumentation,Humans,Models,Muscle Rigidity,Muscle Rigidity: diagnosis,Muscle Rigidity: etiology,Neural Networks (Computer),Parkinson Disease,Parkinson Disease: complications,Parkinson Disease: diagnosis,Parkinson Disease: physiopathology,Proprioception,Proprioception: physiology,Reflex,Spinal Cord,Spinal Cord: physiopathology,Stretch,Stretch: physiology,Time Factors},
