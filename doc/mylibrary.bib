journal = {The American journal of physiology},
number = {1},
pages = {C45--C49},
pmid = {835695},
title = {{Separation of active and passive components of short-range stiffness of muscle.}},
volume = {232},
year = {1977}
}
@article{Bevan2000,
abstract = {Reciprocally connected glutamatergic subthalamic and GABAergic globus pallidus neurons have recently been proposed to act as a generator of low-frequency oscillatory activity in Parkinson's disease. To determine whether GABAA receptor-mediated synaptic potentials could theoretically generate rebound burst firing in subthalamic neurons, a feature that is central to the proposed oscillatory mechanism, we determined the equilibrium potential of GABAA current (E GABAA) and the degree of hyperpolarization required for rebound firing using perforated-patch recording. In the majority of neurons that fired rebounds,E GABAA was equal to or more hyperpolarized than the hyperpolarization required for rebound burst firing. These data suggest that synchronous activity of pallidal inputs could underlie rhythmic bursting activity of subthalamic neurons in Parkinson's disease.},
author = {Bevan, Mark D. and Wilson, Charles J. and Bolam, J. Paul and Magill, Peter J.},
journal = {Journal of Neurophysiology},
month = {may},
