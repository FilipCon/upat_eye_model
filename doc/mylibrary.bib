journal = {IEEE Transactions on Biomedical Engineering},
keywords = {Biomechanics,dissemination,joint,joint mechanics,mechanics,model,musculoskeletal,publishing,repeatability,reproducibility,sharing,simulation,tissue,tissue mechanics},
number = {10},
