isbn = {1554-527X (Electronic)$\backslash$n0736-0266 (Linking)},
issn = {07360266},
journal = {Journal of Orthopaedic Research},
keywords = {and causes clinical problems,avoid edge loading,can lead,ceramic-on-ceramic,edge loading damages hard-on-hard,excessive edge loading wear,for metal-on-metal,hip,hip replacements,implants,metal-on-metal,mom,muscles},
